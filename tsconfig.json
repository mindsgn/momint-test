{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./src",
    "allowJs": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,                     
    "skipLibCheck": true,
    "resolveJsonModule": true,
  "esModuleInterop": true,
  },
  "include": [
    "src/**/*"  
  ],
  "exclude": [
    "./node_modules",
    "./frontend"
  ]
}
